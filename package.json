{
    "name": "@petermetz-fyi/openapi-llm-adapter-gemini",
    "version": "1.0.0",
    "description": "A zero-dependency TypeScript library that automatically maps OpenAPI operations to Gemini Functions declarations.",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "exports": {
        "import": "./dist/index.esm.js",
        "require": "./dist/index.cjs.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build": "tsup src/index.ts --dts --format esm,cjs,iife",
        "prepare": "npm run build",
        "test": "vitest ./test/*.test.ts",
        "example:basic": "run-p 'example:basic:*'",
        "example:basic:main": "tsx ./examples/basic-todo-mvc/main.ts",
        "example:basic:agent": "tsx ./examples/basic-todo-mvc/agent.ts --open-api-specs=./examples/basic-todo-mvc/openapi.json"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "19.2.3",
        "@angular/animations": "19.2.2",
        "@angular/cli": "19.2.3",
        "@angular/common": "19.2.2",
        "@angular/compiler": "19.2.2",
        "@angular/compiler-cli": "19.2.2",
        "@angular/core": "19.2.2",
        "@angular/forms": "19.2.2",
        "@angular/material": "19.2.3",
        "@angular/platform-browser": "19.2.2",
        "@angular/platform-browser-dynamic": "19.2.2",
        "@angular/platform-server": "19.2.2",
        "@angular/router": "19.2.2",
        "@fastify/cors": "11.0.0",
        "@google/generative-ai": "0.24.0",
        "@nguniversal/common": "16.2.0",
        "@nguniversal/express-engine": "16.2.0",
        "@redocly/openapi-core": "1.33.1",
        "@scalar/openapi-types": "0.1.9",
        "@types/node": "22.13.10",
        "@types/yargs": "17.0.33",
        "axios": "1.8.3",
        "body-parser": "1.20.3",
        "concurrently": "9.1.2",
        "cors": "2.8.5",
        "express": "4.21.2",
        "fastify": "5.2.1",
        "npm-run-all2": "7.0.2",
        "openapi-types": "12.1.3",
        "rxjs": "7.8.2",
        "tslib": "2.8.1",
        "tsup": "8.4.0",
        "tsx": "4.19.3",
        "typescript": "5.8.2",
        "vitest": "3.0.8",
        "yargs": "17.7.2",
        "zone.js": "0.15.0"
    },
    "sideEffects": false,
    "license": "Apache-2.0",
    "packageManager": "pnpm@10.6.5"
}
